{
    "name": "amadeus-hos-res-wc-boilerplate",
    "version": "1.0.0",
    "description": "Boilerplate for LitElement based webcomponent development projects. - This seeds the project with initial project structure and the minimum required plugins and coding standards.",
    "private": true,
    "author": "TravelClick",
    "engines": {
        "node": "12.14.1",
        "npm": "6.13.4"
    },
    "scripts": {
        "build": "npm run lint && npm run test && webpack",
        "start": "npm run build && webpack-dev-server",
        "typedoc": "typedoc",
        "test": "karma start",
        "lint": "tslint -c tslint.json 'src/app/**/*.ts'",
        "packagr": "npm run build --prod && npm pack ./dist",
        "start-build-server": "build-storybook && node server/index.js",
        "build-storybook": "build-storybook",
        "storybook-build": "npm run build && node server/server.js && start-storybook -s ./node_modules,./dist,./ -p 9006",
        "storybook": "node server/server.js && start-storybook -s ./node_modules,./dist,./ -p 9006"
    },
    "repository": {
        "type": "git",
        "url": "https://git.travelclick.com/projects/RES/repos/tc-comp-boilerplate/browse"
    },
    "license": "",
    "dependencies": {
        "lit-element": "2.3.1",
        "tc-comp-style": "1.1.0-alpha-18"
    },
    "devDependencies": {
        "@babel/cli": "7.8.4",
        "@babel/core": "7.9.0",
        "@babel/plugin-proposal-class-properties": "7.8.3",
        "@babel/plugin-proposal-decorators": "7.8.3",
        "@babel/preset-env": "7.9.0",
        "@storybook/addon-a11y": "5.3.18",
        "@storybook/addon-actions": "5.3.18",
        "@storybook/addon-knobs": "file:storybook-addon-knobs-5.3.18.tgz",
        "@storybook/addon-viewport": "5.3.18",
        "@storybook/addons": "5.3.18",
        "@storybook/html": "5.3.18",
        "@types/jasmine": "3.5.0",
        "amadeus-hos-res-wc-components": "1.0.0-alpha-12",
        "babel-loader": "8.1.0",
        "boxen": "4.2.0",
        "chalk": "4.0.0",
        "clean-webpack-plugin": "3.0.0",
        "copy-webpack-plugin": "5.1.1",
        "css-loader": "3.4.2",
        "express": "4.17.1",
        "generate-json-webpack-plugin": "0.3.1",
        "html-webpack-plugin": "4.0.3",
        "istanbul-instrumenter-loader": "3.0.1",
        "jasmine": "3.5.0",
        "karma": "4.4.1",
        "karma-chrome-launcher": "3.1.0",
        "karma-coverage-istanbul-reporter": "2.1.1",
        "karma-jasmine": "3.1.0",
        "karma-webpack": "4.0.2",
        "moment": "2.25.1",
        "node-sass": "4.13.1",
        "optimize-css-assets-webpack-plugin": "5.0.3",
        "progress-bar-webpack-plugin": "2.1.0",
        "puppeteer": "2.0.0",
        "request": "2.88.0",
        "request-promise": "4.2.5",
        "sass-loader": "8.0.2",
        "storybook-addon-xd-designs": "5.5.0",
        "tc-comp-services": "1.1.0-alpha-25",
        "terser-webpack-plugin": "2.3.5",
        "to-string-loader": "1.1.6",
        "ts-dedent": "1.1.1",
        "ts-loader": "6.2.2",
        "tslint": "6.1.0",
